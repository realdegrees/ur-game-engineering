{
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.vs": true,
    "**/.gitmodules": true,
    "**/.vsconfig": true,
    "**/*.booproj": true,
    "**/*.pidb": true,
    "**/*.suo": true,
    "**/*.user": true,
    "**/*.userprefs": true,
    "**/*.unityproj": true,
    "**/*.dll": true,
    "**/*.mid": true,
